<h1> Welcome <%= @guest.name %>!</h1>

<h1>Bookings</h1>

<table>
  <thead>
  <tr>
    <th>Cost</th>
    <th>Indate</th>
    <th>Outdate</th>
    <th>Numofguests</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @bookings.each do |booking| %>
    <tr>
      <td><%= booking['cost'] %></td>
      <td><%= booking['inDate'] %></td>
      <td><%= booking['outDate'] %></td>
      <td><%= booking['numOfGuests'] %></td>
      <td><%= link_to 'Edit', edit_guest_booking_path(booking) %></td>
      <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>




